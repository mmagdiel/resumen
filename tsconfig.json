{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "paths": {
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components/"],
      "@services/*": ["./src/services/*"],
      "@services": ["./src/services/"],
      "@models/*": ["./src/models/*"],
      "@models": ["./src/models/"],
      "@stores/*": ["./src/stores/*"],
      "@stores": ["./src/stores/"],
      "@label": ["./labels.json"],
      "@uis/*": ["./src/uis/*"],
      "@/*": [ "./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}