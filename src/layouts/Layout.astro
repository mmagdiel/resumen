---
import Sprites from "@uis/Sprites.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <title>{title}</title>
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width" />
  </head>
  <body>
    <slot />
    <Sprites />
  </body>
</html>
